{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"extends": "../../internal/config/api-extractor.json",
	// the file paths in the shared api-extractor.json don't work for us here, so we specify them manually:
	"mainEntryPointFilePath": "<projectFolder>/packages/namespaced-annotator/.tsbuild-api/index.d.ts",
	"compiler": {
		"tsconfigFilePath": "<projectFolder>/packages/namespaced-annotator/tsconfig.json"
	},
	"apiReport": {
		"reportFolder": "<projectFolder>/packages/namespaced-annotator/",
		"reportTempFolder": "<projectFolder>/packages/namespaced-annotator/api/temp"
	},
	"docModel": {
		"apiJsonFilePath": "<projectFolder>/packages/namespaced-annotator/api/api.json"
	},
	"dtsRollup": {
		"untrimmedFilePath": "<projectFolder>/packages/namespaced-annotator/api/internal.d.ts",
		"publicTrimmedFilePath": "<projectFolder>/packages/namespaced-annotator/api/public.d.ts"
	}
}
